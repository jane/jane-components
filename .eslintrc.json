{
  "root": true,
  "plugins": [
    "jane",
    "flowtype",
    "flowtype-errors"
  ],
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/react",
    "plugin:jane/prettier",
    "plugin:jane/jest",
    "plugin:jane/flow"
  ],
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  }
}
