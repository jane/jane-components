{
  "root": true,
  "plugins": [
    "jane"
  ],
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/react",
    "plugin:jane/jest",
    "plugin:jane/flow",
    "plugin:jane/prettier"
  ],
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  },
  "rules": {
    "flowtype/no-existential-type": 0
  }
}
