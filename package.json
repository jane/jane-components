{
  "name": "jane-components",
  "description": "Jane's React component library",
  "version": "0.1.4",
  "author": "Jane web team",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "run-p -s build:*",
    "build:babel": "babel --ignore \"src/**/*.test.js\" src/ -d lib/",
    "build:rollup": "rollup -c",
    "copy-flow": "flow-copy-source src lib",
    "cover": "npm run test:jest -- --coverage --no-cache",
    "cover:coveralls": "if [ -n \"$COVERALLS_REPO_TOKEN\" ] ; then coveralls < coverage/lcov.info ; fi || true",
    "docs": "styleguidist build",
    "format": "prettier \"{src,config}/**/*.js\" \"*.js\" --write && sortpack",
    "open-coverage": "opn coverage/lcov-report/index.html",
    "postbuild": "run-p -s docs copy-flow",
    "prebuild": "npm run test:build && sortpack",
    "preversion": "npm run build",
    "start": "styleguidist server",
    "test": "run-s -s test:flow test:lint test:jest",
    "test:build": "run-s -s test:lint test:flow cover cover:coveralls",
    "test:flow": "flow check",
    "test:jest": "node config/jest/test.js --env=jsdom",
    "test:lint": "eslint src config .styleguide *.js",
    "update-types": "flow-typed update-cache && flow-typed install --overwrite"
  },
  "homepage": "https://jane.github.io/jane-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/jane/jane-components.git"
  },
  "bugs": "https://github.com/jane/jane-components/issues",
  "keywords": [
    "components",
    "jane",
    "jane-components",
    "react"
  ],
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-styled-name": "2.0.0",
    "classnames": "2.2.6",
    "coveralls": "3.0.3",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "5.16.0",
    "eslint-plugin-jane": "5.0.0",
    "flow-bin": "0.86.0",
    "flow-copy-source": "2.0.3",
    "flow-typed": "2.5.1",
    "husky": "2.1.0",
    "jest": "24.7.1",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.5",
    "mq-polyfill": "1.1.8",
    "npm-run-all": "4.1.5",
    "object-assign": "4.1.1",
    "opn-cli": "4.1.0",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-styleguidist": "8.0.6",
    "react-test-renderer": "16.8.6",
    "rollup": "1.10.1",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "sortpack": "1.1.5",
    "styled-components": "4.2.0",
    "webpack": "4.30.0",
    "whatwg-fetch": "3.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/styled.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ]
  },
  "lint-staged": {
    "ignore": [
      "dist/*.js",
      "lib/**/*.js",
      "docs/**/*.js"
    ],
    "linters": {
      "*.js": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^3.0.0 || ^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
